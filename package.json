{
  "name": "dotonpaper",
  "version": "1.0.0",
  "description": "Track your dots",
  "license": "MIT",
  "author": {
    "name": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kamalmoussa1/dotonpaper"
  },
  "bugs": {
    "url": "https://github.com/kamalmoussa1/dotonpaper/issues"
  },
  "private": true,
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package": "electron-packager . --platform=linux --out ./build --electron-version=2.0.14 --overwrite ",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "paper": "^0.11.5"
  },
  "devDependencies": {
    "electron": "^2.0.14",
    "electron-builder": "^20.38.2",
    "electron-packager": "^12.2.0",
    "electron-reload": "^1.4.0"
  }
}
